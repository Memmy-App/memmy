{
	"extends": "expo/tsconfig.base",
  "compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"strict": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": false,
		"noImplicitReturns": true,
		"baseUrl": ".",
    "paths": {
      "@root/*": [
        "*"
      ],
      "@src/*": [
        "src/*"
      ]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"],
}
