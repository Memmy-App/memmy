{
  "cli": {
    "version": ">= 4.1.2"
  },
  "build": {
    "development": {
			"developmentClient": true,
      "distribution": "internal",
			"env": {
				"EXPO_PUBLIC_IS_TEST_FLIGHT": "false"
			}
    },
    "preview": {
      "distribution": "internal"
    },
		"production-internal": {
			"distribution": "internal",
			"env": {
				"EXPO_PUBLIC_IS_TEST_FLIGHT": "true"
			}
		},
    "production": {
			"env": {
				"EXPO_PUBLIC_IS_TEST_FLIGHT": "false"
			}
		},
		"production-tf": {
			"env": {
				"EXPO_BUILD_IS_TEST_FLIGHT": "true"
			}
		}
  },
  "submit": {
    "production": {}
  }
}
